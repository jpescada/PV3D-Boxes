/** * Class CustomEvent * Desc: evento com object result * View 2007 *  * by Paulo Afonso *  *  *  *  * Exemplo de uso *  * //ao gerar o evento  * objecto.dispatchEvent( new CustomEvent(Event.COMPLETE, {propriedade: valor}) ); *  * //no listener do evento * a.addEventListener( Event.COMPLETE, onComplete ); * public function onComplete( e:EventObjectResult ) * { * 		trace("nota transportada: " + e.note); * } *  *  *  */package lib.events{	import flash.events.Event;		public class CustomEvent extends Event	{		protected var _note:Object;				public function CustomEvent(type:String, note:Object = null, bubbles:Boolean = false, cancelable:Boolean = true)		{			super(type, bubbles, cancelable);			_note = note;			}				public function get note():Object 		{ 			return _note; 		}				public override function clone():Event		{			return new CustomEvent(type, bubbles, cancelable, note);		}			}}